//设置时间轴ul宽度
var timeItemWidth = parseInt($(".historyYear").outerWidth(true));
var liCount = parseInt($(".timerContainer .fk_timeUl li").length);
var timeWidthSet = timeItemWidth * liCount;
$(".timerContainer .fk_timeUl").css("width",timeWidthSet+"px");

function changeH(obj){
	var id=  obj;
	$("#"+id).siblings().removeClass("historyYearCur");
	$("#"+id).addClass("historyYearCur");
	$(".histroyPanel").siblings().removeClass("activeYear");
	$("."+id).addClass("activeYear");
	
	//补充时间轴移动
	var timerUlPos = parseInt($(".timerContainer .fk_timeUl").css("left"));
	var index = $(".historyYearCur").attr("_index");
	var maxItemShow = 7;
	var fristShow = (timerUlPos / timeItemWidth)*-1;
	var lastShow = fristShow + 6 ;
	if(index > lastShow){
		timerUlPos -= timeItemWidth;
		
	}else if(index < fristShow){
		timerUlPos += timeItemWidth;
	}
	$(".timerContainer .fk_timeUl").css("left",timerUlPos+"px")
	
	if(index == 0){
		$(".preBtn").addClass("preBtnUnable");
	}else{
		$(".preBtn").removeClass("preBtnUnable");
	}

	if(index == liCount-1){
		$(".nextBtn").addClass("nextBtnUnable");
	}else{
		$(".nextBtn").removeClass("nextBtnUnable");
	}	

	$('#autoHeightDiv').css("height", 0);
	footAutoHeight();

}

function changeHistoryPanel(opt){
	//opt=1--next    opt=-1---pre
	var index = $(".historyYearCur").attr("_index");
	if(opt == 1){
		if(index == liCount-1){
			return false;
		}else if(index == liCount-2){
			$(".nextBtn").addClass("nextBtnUnable");
		}
		index++;
		var nextId = $(".historyYearCur").next().attr("id");
		changeH(nextId);	
	}

	if(opt == -1){
		if(index == 0){
			return false;
		}else{
			index--;
			var preId = $(".historyYearCur").prev().attr("id");
			changeH(preId);
		}
	}
}


