var JPlaceHolder = {
    //检测
    _check : function(){
        return 'placeholder' in document.createElement('input');
    },
    //初始化
    init : function(){
        if(!this._check()){
            this.fix();
        }
    },
    //修复
    fix : function(){
        jQuery(':input[placeholder]').each(function(index, element) {
            var self = $(this), txt = self.attr('placeholder');
            self.wrap($('<div style="display:inline-block;"></div>').css({position:'relative', zoom:'1', border:'none', background:'none', padding:'none', margin:'none'}));
            var pos = self.position(), h = self.outerHeight(true),h2 = self.outerHeight(true), paddingleft = self.css('padding-left');
            if($(this).attr("id") == "mark"){
            	h2 =70;
            }
            var holder = $('<span></span>').text(txt).css({position:'absolute', left:pos.left, top:pos.top, height:h, lineHeight:h2+"px", paddingLeft:"15px", color:'#aaa'}).appendTo(self.parent());
            self.focusin(function(e) {
                holder.hide();
            }).focusout(function(e) {
                if(!self.val()){
                    holder.show();
                }
            });
            holder.click(function(e) {
                holder.hide();
                self.focus();
            });
        });
    }
};
//执行
if (Fai.isIE8() || Fai.isIE9()){
	jQuery(function(){
	    JPlaceHolder.init();
	});
}

// 刷新验证码
function changeValidateImg(){
	$('#validateCodeImg').attr('src', '/validateCode.jsp?' + parseInt(Math.random() * 1000));
}

// 提交表单
function formSubmit(){
	var name = $.trim($("#name").val());
	var email = $.trim($("#email").val());
	var mobile = $.trim($("#mobile").val());
	var mark = $.trim($("#mark").val());

	if(name.length<=0){
		$(".form_rtMsg").html("姓名不能为空");
		return;
	}
	if(email.length<=0){
		$(".form_rtMsg").html("邮箱不能为空");
		return;
	}
	var isEmail = /^[a-z0-9](\w|\.|-)*@([a-z0-9]+-?[a-z0-9]+\.){1,3}[a-z]{2,4}$/i.test(email);
    if(!isEmail){
    	$(".form_rtMsg").html("请输入正确的邮箱格式");
    	return;
    }
	if(mobile.length<=0){
		$(".form_rtMsg").html("手机不能为空");
		return;
	}
	var isMobile = /^1[3|4|5|8]\d{9}$/.test(mobile);//验证手机号码
    if(!isMobile){
    	$(".form_rtMsg").html("请输入正确的手机号码");
		return;
    }

    var pattern = new RegExp("[~'!#$%^&*()-+_=:<>]");
    if(pattern.test(name) || pattern.test(email) || pattern.test(mobile)){
		$(".form_rtMsg").html("不允许填写特殊字符");
		return;
	}

    var validateCode = $("#validateCode").val();
    if( validateCode == '' ){
		$(".form_rtMsg").html('请输入验证码');
		return;
	}else{
		$(".form_rtMsg").html('');
	}

	$.ajax({
		type:"post",
		url:"../ajax/alliance_h.jsp?cmd=addAliedClient",
		data:{
			email:email,
			name:name,
			mobile:mobile,
			mark:mark,
			validateCode:validateCode,
		},
		error:function(){
			changeValidateImg();
			$(".form_rtMsg").html("服务繁忙，请稍候重试");
		},
		success:function(result){
			result = jQuery.parseJSON(result);
			$(".form_rtMsg").html(result.msg);
			changeValidateImg();
			if(result.success){
				$("#name,#email,#mobile,#mark,#validateCode").val("");
			}
		}
	});
}

// 品牌代理商查询
function queryBrand(){
	var name = $(".brand_name").val();
	if(name.length<2){
		$(".brand_data").html(getBrandHint("最少需要输入2个字符"));
		return;
	}

	$.ajax({
		type:"post",
		url:"../ajax/alliance_h.jsp?cmd=queryBrand",
		data:{
			name:name,
		},
		error:function(){
			$(".brand_data").html(getBrandHint("服务繁忙，请稍候重试"));
		},
		success:function(result){
			result = jQuery.parseJSON(result);

			if(result.success){
				html = "<ul>"
				data = result.data;
				for(var i=0;i<data.length;i++){
					html += "<li><span>公司名称：</span>"+data[i].name+"</li>";
					html += "<li><span>所在地区：</span>"+data[i].province+data[i].city+"</li>";
					html += "<li><span>代理商授权编号：</span>"+data[i].accreditId+"</li>";
					html += "<li><span>授权期限：</span>"+data[i].time+"</li>";
					html += "<li><span>电话号码：</span>"+data[i].phone+"</li>";
					if(i!=data.length-1){
						html += "<li style='border-bottom: #2e82ff 1px dashed; background-color: white; width: 535px; margin-left: 160px;'></li>";
					}
				}
				html+="</ul>"
				$(".brand_data").html(html);
				// 没有找到
				if(data.length==0){
					$(".brand_data").html(getBrandHint("没查到对应品牌代理商信息"));
				}
			}
		}
	});
}

// 返回提示信息
function getBrandHint(msg){
	return '<span style="text-align: left; float: left; margin-left: 160px; font-size: 16px; color: red;">'+msg+'</span>';
};

